<div class="row">
  <div class="col-md-12">
    <div class="box box-solid">
      <div class="box-header">
        <h3 class="box-title"> Qualification Details </h3>
      </div>
      <div class="box-body">
        <div class="row">
          <div class="col-md-5">
        <%= simple_form_for @qualification do |f| %>
          <%=f.error_notification %>
          <%=f.input :name %>
          <%=f.input :sector_id, collection: Sector.all.map{|a| [a.name, a.id] } %>
          

          <%#=f.input :description, as: :text, input_html: { rows: 6 } %>
          <div class="box-footer">
            <%=f.submit "Create Qualification", class: "btn btn-primary btn-flat pull-right" %>
          </div>
        <% end %>
      </div>
    </div>
      </div>
    </div>
  </div>
</div>
