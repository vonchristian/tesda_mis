<div class="row">
        <div class="col-md-3">

          <!-- Profile Image -->
          <div class="box box-primary">
            <div class="box-body box-profile">
              <%#= image_tag(@qualification.avatar.url(:thumb), class: "profile-user-img img-responsive img-circle", alt: "Assessment center logo") %>
              <h3 class="profile-username text-center"><%= @qualification.name %></h3>

              <p class="text-muted text-center"><%#= @qualification.current_address %></p>

              <ul class="list-group list-group-unbordered">
                <li class="list-group-item">
                  <b>NC</b> <a class="pull-right"><%=@qualification.nc_certifieds.count %></a>
                </li>
                <li class="list-group-item">
                  <b>COC</b> <a class="pull-right"><%= @qualification.coc_certifieds.count %> </a>
                </li>
              </ul>

              <!-- <a href="#" class="btn btn-primary btn-block"><b>Follow</b></a> -->
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->

          <!-- About Me Box -->
          
        </div>
        <!-- /.col -->
        <div class="col-md-9">
          <div class="nav-tabs-custom">
            <ul class="nav nav-tabs">
              <li class="active"><a href="#overview" data-toggle="tab">Overview</a></li>
              <li><a href="#nc-certified" data-toggle="tab">Certified (NC)</a></li>
              <li><a href="#coc-certified" data-toggle="tab">Certified (COC)</a></li>
              <li><a href="#settings" data-toggle="tab">Settings</a></li>

            </ul>
            <div class="tab-content">
              <%= render 'qualifications/partials/overview'%>
              <%= render 'qualifications/partials/nc_certifieds'%>
              <%= render 'qualifications/partials/coc_certifieds'%>
              <%= render 'qualifications/partials/settings'%>



            </div>
              <!-- /.tab-pane -->
            </div>
            <!-- /.tab-content -->
          </div>
          <!-- /.nav-tabs-custom -->
        </div>
        <!-- /.col -->
      </div>