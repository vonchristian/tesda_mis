<div class="row">
  <div class="col-md-12">
    <div class="box box-solid">
      <div class="box-header with-border">
        <div class="row">
          <div class="col-md-2">
            <h3 class="box-title"> Trainees <%=Trainee.count %> </h3>
          </div>
          <div class="col-md-7">
          </div>
          <div class="col-md-3">
            <span class="pull-right"><%=link_to "New Trainee", new_trainee_path, class: "btn btn-primary btn-flat" %></span>
          </div>
        </div>
      </div>
      <div class="box-body">
        <div class="table table-responsive">
          <table class="table table-striped">
            <thead>
              <tr>
                <th> Trainee </th>
                <th> Education </th>
                <th> Gender </th>
              </tr>
            </thead>
            <tbody>
              <% @trainees.each do |trainee| %>
                <tr>
                  <td><%= link_to trainee.full_name, trainee %> </td>
                  <td><%#= trainee.education_title%> </td>
                  <td><%= trainee.sex.capitalize %></td>
                </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
