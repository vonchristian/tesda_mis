<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <div class="box box-solid">
      <div class="box-header">
        <h3 class="box-title">  RWAC Upload Details </h3>
      </div>
      <div class="box-body">
        <i class="fa fa-file-excel-o"></i>
<%=simple_form_for @registry, multipart: true do |f| %>
  <%=f.error_notification %>

  <%=f.input :sheet, as: :file, label: false %>
  <div class="box-footer">
  <%=f.submit "Upload RWAC", class: "btn btn-primary pull-right", data: {disable_with: "Uploading File..."} %>
</div>
<% end %>
